<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="ofertix_rabbitmq.connection.class">PhpAmqpLib\Connection\AMQPConnection</parameter>
        <parameter key="ofertix_rabbitmq.exchange_manager.class">Ofertix\RabbitMqBundle\Manager\ExchangeManager</parameter>
    </parameters>

    <services>
        <service id="ofertix_rabbitmq.abstract_connection" class="%ofertix_rabbitmq.connection.class%" abstract="true" />
        <service id="ofertix_rabbitmq.exchange_manager" class="%ofertix_rabbitmq.exchange_manager.class%" />
    </services>
</container>
